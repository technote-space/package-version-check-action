  "license": "MIT",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/package-version-check-action",
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/package-version-check-action.git"
  },
  "bugs": {
    "url": "https://github.com/technote-space/package-version-check-action/issues"
  },
  "main": "lib/main.js",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^1.1.0",
    "@technote-space/filter-github-action": "^0.1.7",
    "@technote-space/github-action-helper": "^0.2.11",
    "replace-in-file": "^4.2.0"
  },
  "devDependencies": {
    "@technote-space/github-action-test-helper": "^0.0.17",
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
