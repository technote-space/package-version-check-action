    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "nock": "^11.6.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
